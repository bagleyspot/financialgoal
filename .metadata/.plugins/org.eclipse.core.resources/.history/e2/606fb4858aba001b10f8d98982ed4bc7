package come.project.financialgoalbe.entities;

import java.math.BigDecimal;
import java.util.Calendar;
import java.util.List;

import javax.persistence.Entity;

import lombok.Getter;
import lombok.Setter;

@Getter
@Setter
@Entity
public class Bond {
	
	Long idBond;
	
	String isin;
	
	String description;
	
	String folder;
	
	BigDecimal price;
	
	BigDecimal quantity;
	
	String currencies;
	
	BigDecimal cedola;
	
	Calendar expire;
	
	String country;
	
	List<EquityDeal> listEquityDeal;


}
